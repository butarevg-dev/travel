rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /images/poi/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null; // uploads by authenticated users; moderated elsewhere
    }
    match /audio/poi/{allPaths=**} {
      allow read: if true;
      allow write: if request.auth != null;
    }
  }
}